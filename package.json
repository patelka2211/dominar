{
  "name": "@patelka2211/dominar",
  "version": "1.2.0",
  "description": "Dominar is a powerful library designed to make it easy for developers to manipulate the Document Object Model (DOM) and create dynamic HTML content. Dominar empowers you to take control of your web development projects like a pro. Unleash the power to dominate the DOM with Dominar.",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc -p ./tsconfig.json && rollup -c && terser ./index.js -c -m -o ./index.js && terser ./Dominar.js -c -m -o ./Dominar.js && rm -rf ./lib/",
    "test": "tsc -p ./tsconfig.json && tsc ./test/index.ts -t es6 && rollup ./test/index.js -o ./test/out.js && rm -rf ./lib/",
    "prepare": "npm run build"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patelka2211/dominar.git"
  },
  "keywords": [
    "Dominar",
    "Dominar.js",
    "front-end",
    "Dominar by KP",
    "Developed by KP",
    "Kartavya Patel",
    "patelka2211",
    "Dynamic content",
    "DOM manipulation",
    "Front-end development",
    "UI development",
    "Lightweight",
    "Event handling",
    "Open source"
  ],
  "author": "Kartavya Patel <patelka2211@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/patelka2211/dominar/issues"
  },
  "homepage": "https://patelka2211.github.io/dominar",
  "devDependencies": {
    "rollup": "^3.25.1",
    "rollup-plugin-dts": "^5.3.0",
    "terser": "^5.17.7",
    "typescript": "^5.1.3"
  }
}